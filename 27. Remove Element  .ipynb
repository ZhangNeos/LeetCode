   "source": [
    "class Solution:\n",
    "    def removeElement(self, nums: List[int], val: int) -> int:\n",
    "        j=len(nums)\n",
    "        for i in range(j-1,-1,-1):\n",
    "            if nums[i]==val:\n",
    "                nums.pop(i)    \n",
    "        return len(nums)"
   ]
  }
